# Python temperature
class temperature:
    def __init__(self,celsius,fahrenheit):
        self.celsius = celsius
        self.fahrenheit = fahrenheit
    def celsius(self):
        celsius = (fahrenheit - 32) * 5/9
        return celsius
    
    def fahrenheit (self):
        fahrenheit = (celsius * 9/5) + 32
        return fahrenheit
    
#object
